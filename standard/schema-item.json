{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Fire Proof Box",
  "type": "object",
	"properties": {
	  "id": {
	    "title": "ID",
	    "description": "Identifier for the item.",
	    "type": "string"
	  },
	  "title": {
	  	"title": "Title",
	  	"description": "Name of the item.",
	    "type": "string",
	    "maxLength": 200
	  },
	  "description": {
	  	"title": "Description",
	  	"description": "Description of the item.",
	    "type": "string"
	  },
	  "date_added": {
	  	"title": "Date added",
	  	"description": "Date the item was added to the box.",
	    "type": "string",
	    "format": "date"
	  }
	},
	"required": [
	  "id",
	  "title",
	  "date_added"
	]
}

